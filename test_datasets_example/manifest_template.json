        "notes": "Photorealistic landscape"
      }
    },
    {
      "file_path": "ai_generated/dalle_object_001.png",
      "true_label": "ai_generated",
      "metadata": {
        "generator": "DALL-E",
        "version": "3",
        "category": "object",
        "prompt": "product photo of a modern coffee maker on white background",
        "resolution": "1024x1024",
        "has_exif": false,
        "notes": "Product-style image"
      }
    },
    {
      "file_path": "authentic/edited_photo_001.jpg",
      "true_label": "authentic",
      "metadata": {
        "source": "Sony A7IV",
        "category": "portrait",
        "has_c2pa": false,
