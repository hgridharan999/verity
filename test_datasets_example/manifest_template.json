{
  "dataset_name": "AI Detection Test Dataset",
  "version": "1.0.0",
  "created": "2025-12-03",
  "description": "Benchmark dataset for testing AI-generated vs authentic image detection",
  "statistics": {
    "total_samples": 100,
    "authentic": 50,
    "ai_generated": 50,
    "generators": {
      "midjourney": 15,
      "stable_diffusion": 15,
      "dalle": 10,
      "firefly": 10
    },
    "categories": {
      "portraits": 30,
      "landscapes": 25,
      "abstract": 20,
      "objects": 25
    }
  },
  "test_cases": [
    {
      "file_path": "authentic/camera_photo_001.jpg",
      "true_label": "authentic",
      "metadata": {
        "source": "Canon EOS R5",
        "category": "portrait",
        "has_c2pa": false,
        "has_exif": true,
        "resolution": "6000x4000",
        "notes": "Professional portrait with natural lighting"
      }
    },
    {
      "file_path": "authentic/iphone_photo_001.jpg",
      "true_label": "authentic",
      "metadata": {
        "source": "iPhone 15 Pro",
        "category": "landscape",
        "has_c2pa": false,
        "has_exif": true,
        "resolution": "4032x3024",
        "notes": "Natural landscape, unedited"
      }
    },
    {
